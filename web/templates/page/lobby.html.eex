<%= if @current_user do %>
<%= content_tag :div, id: "lobby_div", data: [username: @current_user.username] do %>
<div id="wsock_conn_div">
    <wsock-conn></wsock-conn>
</div>
<div id="database_div">
    <database></database>
</div>
<div>
    <div id="sidebar_div">
        <sidebar></sidebar>
    </div>
    <div id="topbar_div">
        <topbar></topbar>
    </div>
    <div id="main_div">
        <main-window></main-window>
    </div>
</div>
<% end %>
<% end %>
